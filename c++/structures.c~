#include <stdio.h>
#include <string.h>

struct Book{

  char title[50];
  char author[50];
  int booknumber;
  
};

int main(){

  struct Book Book1;

  strcpy(Book1.title, "Your Mom");
  strcpy(Book1.author, "Salman Hossain");
  Book1.booknumber = 111102;

  printf("%s\n", Book1.title);
  printf("%s\n", Book1.author);
  printf("%d\n", Book1.booknumber);

  return 0;
}
